{
  "name": "project",
  "version": "0.0.0",
  "private": true,
  "scripts": {
    "start": "DEBUG=project:* nodemon ./bin/www",
    "prettier": "prettier --config .prettierrc.json 'src/**/*.js' --write",
    "lint": "eslint './**/*.js' --fix",
    "create-dev-sql": "node node_modules/db-migrate/bin/db-migrate create store --sql-file",
    "create-test-sql": "node node_modules/db-migrate/bin/db-migrate create store_test --sql-file",
    "dev-migrate": "node node_modules/db-migrate/bin/db-migrate up -e dev",
    "test-migrate": "node node_modules/db-migrate/bin/db-migrate up -e test",
    "reset-dev-db": "node node_modules/db-migrate/bin/db-migrate reset -e dev",
    "reset-test-db": "node node_modules/db-migrate/bin/db-migrate reset -e test",
    "test": "db-migrate --env test up && ENV=test jasmine || db-migrate --env test down",
    "serverstart": "node ./bin/www"
  },
  "dependencies": {
    "async": "^3.2.4",
    "bcrypt": "^5.1.0",
    "bcryptjs": "^2.4.3",
    "bg": "^1.0.0",
    "body-parser": "^1.20.2",
    "compression": "^1.7.4",
    "config": "^3.3.9",
    "cookie-parser": "~1.4.4",
    "cors": "^2.8.5",
    "db-migrate": "^0.11.13",
    "db-migrate-pg": "^1.3.0",
    "debug": "~2.6.9",
    "dotenv": "^16.0.3",
    "ejs": "^3.1.9",
    "express": "^4.18.2",
    "express-validator": "^7.0.1",
    "helmet": "^6.1.5",
    "html-to-text": "^9.0.5",
    "http-errors": "~1.6.3",
    "joi": "^17.9.2",
    "jsonwebtoken": "^9.0.0",
    "luxon": "^3.3.0",
    "morgan": "~1.9.1",
    "mysql": "^2.18.1",
    "node-fetch": "^3.3.1",
    "nodemailer": "^6.9.3",
    "prisma": "^4.15.0",
    "pug": "^3.0.2",
    "stylus": "^0.54.5"
  },
  "devDependencies": {
    "copyfiles": "^2.4.1",
    "eslint": "^8.39.0",
    "nodemon": "^2.0.22",
    "prettier": "^2.8.8"
  }
}
